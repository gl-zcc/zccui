{
  "name": "zccui",
  "version": "0.1.2",
  "private": false,
  "author": "gl-zcc",
  "license": "MIT",
  "homepage": "https://github.com/gl-zcc/zccui",
  "repository": {
    "type": "git",
    "url": "git@github.com:gl-zcc/zccui.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "React",
    "Component"
  ],
  "description": "A library of react components, which mainly stores components that can be reused by all business lines of AI-Indeeded Company.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",

  "scripts": {
    "clean": "rimraf ./dist",
    "build:ts": "tsc -p tsconfig.build.json",
    "build:css": "lessc ./src/components/button/index.css ./dist/button/index.css",
    "build": "npm run clean && npm run build:ts && npm run build:css",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}